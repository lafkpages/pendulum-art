{
	"plugins": ["prettier-plugin-svelte", "@ianvs/prettier-plugin-sort-imports"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"importOrder": [
		"<TYPES>^[.]",
		"<TYPES>",
		"",
		"^svelte$",
		"^svelte/",
		"^@sveltejs/",
		"",
		"<BUILTIN_MODULES>",
		"",
		"<THIRD_PARTY_MODULES>",
		"",
		"^[.$]"
	],
	"importOrderTypeScriptVersion": "5.4.2"
}
